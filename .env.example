# Environment Variables Configuration
# Copy this file to .env and fill in the actual values

# Server Configuration
PORT=5000

# MongoDB Connection
# Replace with your actual MongoDB connection string
# For local MongoDB: mongodb://localhost:27017/your_database_name
# For MongoDB Atlas: mongodb+srv://<username>:<password>@cluster.mongodb.net/your_database
MONGODB_URI=your_mongodb_connection_string_here

# JWT Secret Key
# IMPORTANT: Use a strong secret key (at least 32 characters)
# You can generate one using: node -e "console(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET=your_super_secret_jwt_key_min_32_characters_here

# Frontend URLs (for CORS)
# Replace with your actual frontend URLs
FRONTEND_URL=https://clienttesting.vervel.app
ADMIN_URL=https://admintesting.vercel.app

